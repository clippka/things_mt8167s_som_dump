<permissions>
    <!-- Marks this device as an iot device. -->
    <feature name="android.hardware.type.embedded" />

    <!-- Add our com.google.android.things library. -->
    <library name="com.google.android.things"
             file="/system/framework/com.google.android.things.jar" />

    <!-- Add our oem_install library. This is used for unit tests but
         currently not the oem_install command line utility. -->
    <library name="oem_install"
             file="/system/framework/oem_install.jar" />
</permissions>
